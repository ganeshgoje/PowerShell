Login-AzureRmAccount 

## Use following statement If you have multiple subscriptions
## Set AzureRmContext -Subscrpition "Developer Program Benifit.

Set-AzureRmContext -SubscriptionName "Developer Program Benefit"

$resourceGroup="Day2"

##Create a new Resrouce Group if it is already not there
New-AzureRmResourceGroup -name "Day2" -Location "Southeast Asia"

$frontendSubnet=New-AzureRmVirtualNetworkSubnetConfig -Name "FrontEndSubnet" `
                        -AddressPrefix "10.0.1.0/24" 
                       
$backendSubnet=New-AzureRmVirtualNetworkSubnetConfig -Name "BackEndSubnet" `
                        -AddressPrefix "10.0.2.0/24" 

New-AzureRmVirtualNetwork -Name "VNET1" `
                        -ResourceGroupName $resourceGroup `
                        -AddressPrefix "10.0.0.0/16" `
                        -Subnet $frontendSubnet, $backendSubnet `
                        -Location "SouthEast Asia"
